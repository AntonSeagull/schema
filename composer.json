{
    "name": "shm/schema",
    "description": "Extensible PHP schema definition library with validation, normalization, and GraphQL type casting",
    "keywords": [
        "schema",
        "validation",
        "normalization",
        "graphql",
        "php",
        "data",
        "structure"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Seagull",
            "email": "hi@antonseagull.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shm\\": "src/",
            "Lumus\\": "src/Lumus/"
        }
    },
    "require": {
        "php": ">=8.1",
        "aws/aws-sdk-php": "3.269.8",
        "sentry/sdk": "*",
        "nette/php-generator": "^4.1",
        "mongodb/mongodb": "*",
        "filp/whoops": "*",
        "poliander/cron": "*",
        "gumlet/php-image-resize": "^2.0",
        "php-ffmpeg/php-ffmpeg": "*",
        "maximal/audio-waveform": "*",
        "kornrunner/blurhash": "*",
        "predis/predis": "^3.0",
        "php-school/cli-menu": "^4.3",
        "geoip2/geoip2": "^3.2",
        "jenssegers/blade": "^2.0",
        "griffinledingham/php-apple-signin": "^1.1",
        "nette/mail": "^4.0"
    },
    "prefer-stable": true
}
